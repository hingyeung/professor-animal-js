    "body-parser": "^1.18.3",
    "dialogflow-fulfillment": "^0.5.0",
    "express": "^4.16.3",
    "lodash": "^4.17.4",
    "npm": "^6.9.0",
    "q": "^1.5.0",
    "serverless-http": "^1.6.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "lambda-local": "^1.4.2",
    "mocha": "^5.2.0",
    "proxyquire": "^1.8.0",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "sinon-promise": "^0.1.3"
  },
